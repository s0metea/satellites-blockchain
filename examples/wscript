# -*- Mode: python; py-indent-offset: 4; indent-tabs-mode: nil; coding: utf-8; -*-

def build(bld):
    if not bld.env['ENABLE_EXAMPLES']:
        return;
    obj = bld.create_ns3_program('sat-net-device-example', ['sat-net-device', 'applications', 'internet'])
    obj.source = 'sat-net-device-example.cc'
